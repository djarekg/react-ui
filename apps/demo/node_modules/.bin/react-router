#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@react-router+dev@7.7.0_@react-router+serve@7.7.0_react-router@7.7.0_react-dom@19.1.0_r_6936af0f36dd0d53827545ba0342dcc1/node_modules/@react-router/dev/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@react-router+dev@7.7.0_@react-router+serve@7.7.0_react-router@7.7.0_react-dom@19.1.0_r_6936af0f36dd0d53827545ba0342dcc1/node_modules/@react-router/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@react-router+dev@7.7.0_@react-router+serve@7.7.0_react-router@7.7.0_react-dom@19.1.0_r_6936af0f36dd0d53827545ba0342dcc1/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@react-router+dev@7.7.0_@react-router+serve@7.7.0_react-router@7.7.0_react-dom@19.1.0_r_6936af0f36dd0d53827545ba0342dcc1/node_modules/@react-router/dev/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@react-router+dev@7.7.0_@react-router+serve@7.7.0_react-router@7.7.0_react-dom@19.1.0_r_6936af0f36dd0d53827545ba0342dcc1/node_modules/@react-router/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@react-router+dev@7.7.0_@react-router+serve@7.7.0_react-router@7.7.0_react-dom@19.1.0_r_6936af0f36dd0d53827545ba0342dcc1/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@react-router/dev/bin.js" "$@"
else
  exec node  "$basedir/../@react-router/dev/bin.js" "$@"
fi
