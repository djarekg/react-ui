#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/esm/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/cli/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules/@graphql-codegen/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/@graphql-codegen+cli@5.0.7_@types+node@24.0.15_graphql@16.11.0_typescript@5.8.3/node_modules:/Users/MasterGriffith/Documents/repos/djarekg/react-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
else
  exec node  "$basedir/../@graphql-codegen/cli/esm/bin.js" "$@"
fi
